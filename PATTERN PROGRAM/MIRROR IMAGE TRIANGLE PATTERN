public class Pattern{
public static void main(String[]args){
for(int i=1;i<=9;i++){
if(i<=5){
for(int j=1;j<=i-1;j++){
System.out.print(" ");
}
for(int j=i;j<=5;j++){
System.out.print(j+ " ");
}
System.out.println();
}else{
int k=10-i;
for(int j=1;j<=k-1;j++){
System.out.print(" ");
}
for(int j=k;j<=5;j++){
System.out.print(j+ " ");
}

System.out.println();
}
}}}
